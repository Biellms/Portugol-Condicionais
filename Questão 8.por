programa {
	
	funcao inicio() {

		inteiro N
	
		escreva("\n Digite um valor: ")
		leia(N)

		se (N > 100) {
			escreva("\n A variavel ",N," é maior que 100!!")	
		}
		senao se(N <= 100) {
			escreva("\n A variavel ",N," é menor ou igual a 100\n E agora valera ",N=0)
		}
		senao {
			escreva("\n VALOR INCORRETO!!")	
		}
	}
}
